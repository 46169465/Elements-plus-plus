{
  "name": "elements-plus-plus",
  "version": "1.0.0",
  "homepage": "https://github.io/ZhangXusen/Elements-plus-plus",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:components": "node ./command/build.js",
    "lib": "npm run build:components && cp package.json",
    "prettier": "prettier --write .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint:script": "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./",
    "clean": "rimraf **/*/node_modules",
    "lint:eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 100 --fix",
    "lint:prettier": "prettier --write  **/*.{js,ts,json,tsx,css,scss,vue,html}",
    "lint:stylelint": "stylelint  --fix \"**/*.{html,vue,css,scss}\" ",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "prepare": "husky",
    "cmt": " git-cz"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.10",
    "@types/mockjs": "^1.0.6",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue-macros/volar": "^0.17.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "cz-git": "^1.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.31.0",
    "fs-extra": "^11.1.1",
    "gh-pages": "^5.0.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^2.8.8",
    "sass": "^1.54.2",
    "sass-loader": "^13.2.2",
    "stylelint": "^16.10.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^4.6.4",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
